{
  "author": {
    "email": "gajus@gajus.com",
    "name": "Gajus Kuizinas",
    "url": "http://gajus.com"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "test/gcs-cache/**/*"
    ],
    "ignoredByWatcher": [
      "test/fixtures/**/*"
    ],
    "require": [
      "ts-node/register/transpile-only"
    ]
  },
  "dependencies": {
    "@actions/core": "^1.8.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.0.1",
    "@actions/glob": "^0.3.0",
    "@google-cloud/storage": "^5.19.4",
    "semver": "^7.3.5",
    "tmp-promise": "^3.0.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/node": "^17.0.22",
    "@types/sinon": "^10.0.11",
    "ava": "^4.1.0",
    "eslint": "^8.11.0",
    "eslint-config-canonical": "^33.0.1",
    "husky": "^7.0.2",
    "rollup": "^2.72.1",
    "sinon": "^13.0.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test && npm run build"
    }
  },
  "keywords": [
    "github",
    "action",
    "google",
    "storage"
  ],
  "license": "BSD-3-Clause",
  "main": "./dist/src/index.js",
  "name": "gcs-cache",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/gajus/gcs-cache"
  },
  "scripts": {
    "build": "rollup -c --input src/main.ts --file action/main.js && rollup -c --input src/post.ts --file action/post.js",
    "lint": "eslint ./src ./test && tsc --noEmit",
    "test": "ROARR_LOG=1 NODE_ENV=test ava --serial --verbose"
  },
  "version": "1.0.0"
}
